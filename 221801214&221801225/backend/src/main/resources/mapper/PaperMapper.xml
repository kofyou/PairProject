<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="jiekou.demo.mapper.PaperMapper">
    <select id="findAll" resultType="jiekou.demo.entity.Paper">
        SELECT * FROM paper
    </select>
    <select id="searchPage" resultType="jiekou.demo.entity.Paper">
        SELECT * FROM paper WHERE paper_id=#{id}
    </select>
</mapper>